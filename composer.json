{
    "name": "wieni/wmbert",
    "type": "drupal-module",
    "description": "A more user-friendly entity reference list field widget for Drupal 8.",
    "license": "MIT",
    "authors": [
        {
            "name": "Wieni",
            "email": "info@wieni.be"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "drupal/core": "^9.2 || ^10.0"
    },
    "require-dev": {
        "composer-runtime-api": "^2.0",
        "ergebnis/composer-normalize": "^2.0",
        "wieni/wmcodestyle": "^1.0"
    },
    "minimum-stability": "alpha",
    "prefer-stable": true,
    "scripts": {
        "coding-standards": [
            "@composer normalize",
            "php-cs-fixer fix --config=.php_cs.php"
        ]
    }
}
